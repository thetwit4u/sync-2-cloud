{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Sync2Bucket",
  "version": "0.1.0",
  "identifier": "com.exad.sync2bucket",
  "build": {
    "frontendDist": "../out",
    "devUrl": "http://localhost:3000",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "Sync2Bucket",
        "width": 900,
        "height": 700,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "transparent": true,
        "decorations": true,
        "center": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "macOS": {
      "minimumSystemVersion": "10.15",
      "signingIdentity": "Developer ID Application: ExAd (V72M7CT7PD)",
      "hardenedRuntime": true,
      "entitlements": null
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": "http://timestamp.digicert.com"
    },
    "nsis": {
      "oneClick": false,
      "allowUserToChooseInstallDir": true,
      "installerIcon": "icons/icon.ico",
      "headerImage": null,
      "sidebarImage": null,
      "license": null
    }
  }
}
